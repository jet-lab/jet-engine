{
  "name": "@jet-lab/jet-engine",
  "version": "0.1.0",
  "description": "Library for interacting with the Jet on-chain programs",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --excludePrivate --includeVersion src/index.ts",
    "fmt:write": "prettier . --write",
    "fmt:check": "prettier . --check",
    "lint": "eslint src/ --ext .ts",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.13.0",
    "@project-serum/serum": "^0.13.33",
    "@solana/buffer-layout": "^3.0.0",
    "@solana/web3.js": "^1.20.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "prettier": "^2.3.2",
    "typedoc": "^0.22.6",
    "typescript": "^4.3.5"
  },
  "files": [
    "./bin/*",
    "./lib/*"
  ],
  "directories": {
    "lib": "lib"
  }
}
